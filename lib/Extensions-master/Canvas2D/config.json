{
    "build": {
        "outputDirectory": "dist/preview release",
        "srcOutputDirectory": "src/"
    },
    "modules": [
        "canvas2D"
    ],
    "canvas2D": {
        "libraries": [
            {
                "files": [
                    "src/Engine/babylon.dynamicFloatArray.ts",
                    "src/Engine/babylon.rectPackingMap.ts",
                    "src/Engine/babylon.mapTexture.ts",
                    "src/Tools/babylon.math2D.ts",
                    "src/Tools/babylon.IPropertyChanged.ts",
                    "src/Tools/babylon.c2dlogging.ts",
                    "src/Tools/babylon.observableArray.ts",
                    "src/Tools/babylon.observableStringDictionary.ts",
                    "src/Engine/babylon.fontTexture.ts",
                    "src/Engine/babylon.bounding2d.ts",
                    "src/Engine/babylon.primitiveCollisionManager.ts",
                    "src/Engine/babylon.canvas2dLayoutEngine.ts",
                    "src/Engine/babylon.brushes2d.ts",
                    "src/Engine/babylon.smartPropertyPrim.ts",
                    "src/Engine/babylon.prim2dBase.ts",
                    "src/Engine/babylon.modelRenderCache.ts",
                    "src/Engine/babylon.renderablePrim2d.ts",
                    "src/Engine/babylon.shape2d.ts",
                    "src/Engine/babylon.group2d.ts",
                    "src/Engine/babylon.wireFrame2d.ts",
                    "src/Engine/babylon.rectangle2d.ts",
                    "src/Engine/babylon.ellipse2d.ts",
                    "src/Engine/babylon.sprite2d.ts",
                    "src/Engine/babylon.atlasPicture.ts",
                    "src/Engine/babylon.text2d.ts",
                    "src/Engine/babylon.lines2d.ts",
                    "src/Engine/babylon.canvas2d.ts",
                    "src/Engine/babylon.worldSpaceCanvas2dNode.ts",
                    "src/GUI/babylon.gui.UIElement.ts",
                    "src/GUI/Layouts/babylon.gui.stackPanel.ts",
                    "src/GUI/babylon.gui.control.ts",
                    "src/GUI/babylon.gui.contentControl.ts",
                    "src/GUI/babylon.gui.window.ts",
                    "src/GUI/babylon.gui.label.ts",
                    "src/GUI/babylon.gui.button.ts",
                    "src/babylon.d.ts"
                ],
                "shaderFiles": [
                    "/src/shaders/**.fx",
                    "!/src/shaders/**.js.fx"
                ],
                "output": "babylon.canvas2d.js"
            }
        ],
        "build": {
            "srcOutputDirectory": "/src/",
            "distOutputDirectory": "/"
        }
    }
}
